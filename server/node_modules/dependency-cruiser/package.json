		"enhanced-resolve": "5.18.4",
		"ignore": "7.0.5",
		"interpret": "3.1.1",
		"is-installed-globally": "1.0.0",
		"json5": "2.2.3",
		"memoize": "10.2.0",
		"picomatch": "4.0.3",
		"prompts": "2.4.2",
		"rechoir": "0.8.0",
		"safe-regex": "2.1.1",
		"semver": "7.7.3",
		"tsconfig-paths-webpack-plugin": "4.2.0",
		"watskeburt": "5.0.0"
